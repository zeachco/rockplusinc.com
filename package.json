    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-skylight": "^0.4.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
